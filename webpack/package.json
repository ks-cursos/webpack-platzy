{
  "name": "curso-webpack",
  "version": "1.0.0",
  "description": "Caos de uso",
  "main": "index.js",
  "scripts": {
    "env": "env",
    "test": "Hola Mundo",
    "build": "webpack index.js --output bundle.js --mode development",
    "build:local": "webpack",
    "build:external": "webpack --config ./external/webpack.config.js",
    "build:css": "webpack --config ./css-style-loader/webpack.config.js",
    "build:extract:css": "webpack --config ./plugin-extract-text/webpack.config.js",
    "build:extract:multiple": "webpack --config ./multiple-entry-points/webpack.config.js",
    "build:devserver": "webpack-dev-server --config ./webpack-dev-server/webpack.config.js",
    "build:babel": "webpack --config ./babel-loader/webpack.config.js"
  },
  "keywords": [
    "webpack"
  ],
  "author": "FReddy Castillo7",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/preset-env": "7.2.0",
    "babel-loader": "8.0.4",
    "css-loader": "2.0.0",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "style-loader": "0.23.1",
    "webpack": "4.27.0",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.10"
  },
  "dependencies": {
    "npm": "6.5.0",
    "to": "0.2.9",
    "update": "0.7.4"
  }
}
